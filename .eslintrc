{
  "extends": ["react-app", "prettier"],
  "plugins": ["react-hooks", "@typescript-eslint"],
  "overrides": [
    {
      "files": ["**/*.ts?(x)"],
      "rules": {
        "react-hooks/exhaustive-deps": 0,
        "member-access": 0,
        "object-literal-sort-keys": 0,
        "no-console": 0,
        "no-implicit-dependencies": 0,
        "interface-name": 0,
        "no-unused-vars": "error",
        "quotes": ["error", "single"]
      }
    }
  ],
  "parser": "@typescript-eslint/parser"
}
